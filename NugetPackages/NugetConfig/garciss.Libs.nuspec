<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <id>garciss.Libs</id>
    <title>CSharp Libs</title>
    <version>1.0</version>
    <authors>Asier Garcia</authors>
    <owners>Asier Garcia</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Libreria para la gestion de bases de datos como SQLite</description>
    <repository url="https://github.com/g4rc1ss/CsharpLibs" />
    <dependencies>
      <group targetFramework=".NETStandard2.0">
        <dependency id="System.Data.SQLite.Core" version="1.0.111" exclude="Build,Analyzers" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="..\PackagesCompilaciones\netstandard*\*.dll" target="lib\NetStandard\" />
    <file src="..\PackagesCompilaciones\netstandard*\*.pdb" target="lib\NetStandard\" />
    <file src="..\PackagesCompilaciones\netstandard*\*.xml" target="lib\NetStandard\" />
  </files>
</package>